		"tsConfigPath": "./apps/backend/tsconfig.json",
		"useTsNode": true
	},
	"name": "nna",
	"private": true,
	"scripts": {
		"backend:build": "nx build backend",
		"backend:compodoc": "npm run backend:compodoc:build -- -r 3080 -s",
		"backend:compodoc:build": "npx @compodoc/compodoc -p ./apps/backend/tsconfig.doc.json -a .badges/ -d ./dist/documenation/backend",
		"backend:compodoc:coverage": "npm run backend:compodoc:build -- --coverageTest 75 --silent",
		"backend:e2e": "nx e2e backend-e2e --runInBand",
		"backend:e2e:lint": "nx lint backend-e2e",
		"backend:e2e:lint:fix": "npm run backend:e2e:lint -- --fix",
		"backend:e2e:watch": "npm run backend:e2e -- --watch",
		"backend:lint": "nx lint backend",
		"backend:lint:fix": "npm run backend:lint -- --fix",
		"backend:start": "nx serve backend",
		"backend:test": "nx test backend --runInBand",
		"backend:test:watch": "npm run backend:test -- --watch",
		"build": "nx run-many --target=build --parallel=3",
		"compodoc:coverage": "npm run backend:compodoc:coverage && npm run frontend:compodoc:coverage && npm run office:compodoc:coverage",
		"frontend:build": "nx build frontend",
		"frontend:compodoc": "npm run frontend:compodoc:build -- -r 4080 -s",
		"frontend:compodoc:build": "npx @compodoc/compodoc -p ./apps/frontend/tsconfig.doc.json -a .badges/ -d ./dist/documenation/frontend",
		"frontend:compodoc:coverage": "npm run frontend:compodoc:build -- --coverageTest 75 --silent",
		"frontend:e2e": "nx e2e frontend-e2e",
		"frontend:e2e:lint": "nx lint frontend-e2e",
		"frontend:e2e:lint:fix": "npm run frontend:e2e:lint -- --fix",
		"frontend:e2e:watch": "npm run frontend:e2e -- --watch",
		"frontend:i18n:extract": "ngx-translate-extract --input ./apps/frontend/src --output ./apps/frontend/src/locale/{en,fr}.json --key-as-default-value --clean --format namespaced-json && npx eslint ./apps/frontend/src/locale/*.json --fix",
		"frontend:lint": "npm run frontend:lint:code; CODE=$?; npm run frontend:lint:style && [ $CODE -eq 0 ]",
		"frontend:lint:code": "nx lint frontend",
		"frontend:lint:code:fix": "npm run frontend:lint:code -- --fix",
		"frontend:lint:fix": "npm run frontend:lint:code:fix; npm run frontend:lint:style:fix",
		"frontend:lint:style": "nx stylelint frontend",
		"frontend:lint:style:fix": "npm run frontend:lint:style -- --fix",
		"frontend:start": "nx serve frontend",
		"frontend:start:prod": "npm run frontend:start -- --prod",
		"frontend:storybook": "nx storybook frontend",
		"frontend:storybook:build": "nx storybook-build frontend",
		"frontend:storybook:generate": "nx g @nx/angular:stories frontend --generateCypressSpecs false",
		"frontend:test": "nx test frontend",
		"frontend:test:watch": "npm run frontend:test -- --watch",
		"i18n:extract": "npm run libs:ng:i18n:extract && npm run frontend:i18n:extract && npm run office:i18n:extract",
		"libs:common:lint": "nx lint common",
		"libs:common:lint:fix": "npm run libs:common:lint -- --fix",
		"libs:common:test": "nx test common",
		"libs:common:test:watch": "npm run libs:common:test -- --watch",
		"libs:ng:i18n:extract": "ngx-translate-extract --input ./libs/ng/src --output libs/ng/src/lib/translation/locale/{en,fr}.json --key-as-default-value --clean --format namespaced-json && npx eslint ./libs/ng/src/lib/translation/locale/*.json --fix",
		"libs:ng:lint": "nx lint ng",
		"libs:ng:lint:fix": "npm run libs:ng:lint -- --fix",
		"libs:ng:test": "nx test ng",
		"lint": "npm run lint:code; C1=$?; npm run lint:style; C2=$?; npm run lint:other && [ $C1 -eq 0 ] && [ $C2 -eq 0 ]",
		"lint:code": "nx run-many --all --target=lint",
		"lint:code:fix": "npm run lint:code -- --fix",
		"lint:fix": "npm run lint:code:fix; npm run lint:style:fix; npm run lint:other:fix",
		"lint:other": "npx eslint . --ignore-pattern apps/ --ignore-pattern libs/",
		"lint:other:fix": "npm run lint:other -- --fix",
		"lint:style": "stylelint \"./**/*.{css,scss}\"",
		"lint:style:fix": "npm run lint:style -- --fix",
		"office:build": "nx build office",
		"office:compodoc": "npm run office:compodoc:build -- -r 5080 -s",
		"office:compodoc:build": "npx @compodoc/compodoc -p ./apps/office/tsconfig.doc.json -a .badges/ -d ./dist/documenation/office",
		"office:compodoc:coverage": "npm run office:compodoc:build -- --coverageTest 75 --silent",
		"office:e2e": "nx e2e office-e2e",
		"office:e2e:lint": "nx lint office-e2e",
		"office:e2e:lint:fix": "npm run office:e2e:lint -- --fix",
		"office:e2e:watch": "npm run office:e2e -- --watch",
		"office:i18n:extract": "ngx-translate-extract --input ./apps/office/src --output ./apps/office/src/locale/{en,fr}.json --key-as-default-value --clean --format namespaced-json && npx eslint ./apps/office/src/locale/*.json --fix",
		"office:lint": "npm run office:lint:code; CODE=$?; npm run office:lint:style && [ $CODE -eq 0 ]",
		"office:lint:code": "nx lint office",
		"office:lint:code:fix": "npm run office:lint:code -- --fix",
		"office:lint:fix": "npm run office:lint:code:fix; npm run office:lint:style:fix",
		"office:lint:style": "nx stylelint office",
		"office:lint:style:fix": "npm run office:lint:style -- --fix",
		"office:start": "nx serve office",
		"office:start:prod": "npm run office:start -- --prod",
		"office:storybook": "nx storybook office",
		"office:storybook:build": "nx storybook-build office",
		"office:storybook:generate": "nx g @nx/angular:stories office --generateCypressSpecs false",
		"office:test": "nx test office",
		"office:test:watch": "office:test -- --watch",
		"postinstall": "ts-node tools/npm/postinstall.ts",
		"test": "nx run-many --target=test --parallel=3 --runInBand",
		"test:e2e": "nx run-many --target=e2e --parallel=false --runInBand"
	},
	"version": "0.1.0"
