	"scripts": {
		"backend:build": "nx build backend",
		"backend:compodoc": "nx run backend:compodoc",
		"backend:compodoc:build": "nx run backend:compodoc:build",
		"backend:compodoc:coverage": "nx run backend:compodoc:coverage",
		"backend:e2e": "nx e2e backend-e2e --runInBand",
		"backend:e2e:lint": "nx lint backend-e2e",
		"backend:e2e:lint:fix": "npm run backend:e2e:lint -- --fix",
		"backend:e2e:watch": "npm run backend:e2e -- --watch",
		"backend:lint": "nx lint backend",
		"backend:lint:fix": "npm run backend:lint -- --fix",
		"backend:start": "nx serve backend",
		"backend:test": "nx run backend:test --runInBand",
		"backend:test:watch": "npm run backend:test -- --watch",
		"build": "nx run-many --target=build --parallel=3",
		"compodoc": "nx run-many --target=compodoc --output-style=stream",
		"compodoc:build": "nx run-many --target=compodoc --configuration build",
		"compodoc:coverage": "nx run-many --target=compodoc --configuration coverage",
		"frontend:build": "nx build frontend",
		"frontend:compodoc": "nx run frontend:compodoc",
		"frontend:compodoc:build": "nx run frontend:compodoc:build",
		"frontend:compodoc:coverage": "nx run frontend:compodoc:coverage",
		"frontend:e2e": "nx e2e frontend-e2e",
		"frontend:e2e:lint": "nx lint frontend-e2e",
		"frontend:e2e:lint:fix": "npm run frontend:e2e:lint -- --fix",
		"frontend:e2e:watch": "npm run frontend:e2e -- --watch",
		"frontend:i18n:extract": "ngx-translate-extract --input ./apps/frontend/src --output ./apps/frontend/src/locale/en.json --null-as-default-value --clean --format namespaced-json && npx eslint ./apps/frontend/src/locale/*.json --fix",
		"frontend:lint": "npm run frontend:lint:code; CODE=$?; npm run frontend:lint:style && [ $CODE -eq 0 ]",
		"frontend:lint:code": "nx lint frontend",
		"frontend:lint:code:fix": "npm run frontend:lint:code -- --fix",
		"frontend:lint:fix": "npm run frontend:lint:code:fix; npm run frontend:lint:style:fix",
		"frontend:lint:style": "nx stylelint frontend",
		"frontend:lint:style:fix": "npm run frontend:lint:style -- --fix",
		"frontend:start": "nx serve frontend",
		"frontend:start:prod": "npm run frontend:start -- --prod",
		"frontend:storybook": "nx storybook frontend",
		"frontend:storybook:build": "nx storybook-build frontend",
		"frontend:storybook:generate": "nx g @nx/angular:stories ng --interactionTests && nx g @nx/angular:stories frontend --interactionTests",
		"frontend:storybook:test": "nx storybook-test frontend",
		"frontend:storybook:test:watch": "npm run frontend:storybook:test -- --watch",
		"frontend:test": "nx run frontend:test",
		"frontend:test:watch": "npm run frontend:test -- --watch",
		"i18n:extract": "npm run libs:ng:i18n:extract && npm run frontend:i18n:extract",
		"libs:common:lint": "nx lint common",
		"libs:common:lint:fix": "npm run libs:common:lint -- --fix",
		"libs:common:test": "nx run common:test",
		"libs:common:test:watch": "npm run libs:common:test -- --watch",
		"libs:ng:i18n:extract": "ngx-translate-extract --input ./libs/ng/src --output libs/ng/src/lib/translation/locale/en.json --null-as-default-value --clean --format namespaced-json && npx eslint ./libs/ng/src/lib/translation/locale/*.json --fix",
		"libs:ng:lint": "nx lint ng",
		"libs:ng:lint:fix": "npm run libs:ng:lint -- --fix",
		"libs:ng:test": "nx run ng:test",
		"libs:ng:test:watch": "npm run libs:ng:test -- --watch",
		"lint": "npm run lint:code; C1=$?; npm run lint:style; C2=$?; npm run lint:other && [ $C1 -eq 0 ] && [ $C2 -eq 0 ]",
		"lint:code": "nx run-many --all --target=lint",
		"lint:code:fix": "npm run lint:code -- --fix",
		"lint:fix": "npm run lint:code:fix; npm run lint:style:fix; npm run lint:other:fix",
		"lint:other": "npx eslint . --ignore-pattern apps/ --ignore-pattern libs/",
		"lint:other:fix": "npm run lint:other -- --fix",
		"lint:style": "npm run frontend:lint:style",
		"lint:style:fix": "npm run frontend:lint:style:fix",
		"postinstall": "ts-node tools/npm/postinstall.ts",
		"test": "nx run-many --target=test --parallel=4 --runInBand --coverage",
		"test:ci": "nx run-many --target=test --configuration ci --ci --parallel=4 --runInBand",
		"test:e2e": "nx run-many --target=e2e --parallel=false --runInBand"
	},
	"version": "0.3.5"
}
